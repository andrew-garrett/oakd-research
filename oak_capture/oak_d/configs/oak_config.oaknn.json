{
    "oakPipeline": {
        "fps": 20,
        "display": {
            "useDisplay": false,
            "resolution": [640, 360]
        },
        "rgb": {
            "useRGB": true,
            "resolution": "THE_1080_P"
        },
        "depth": {
            "useDepth": true,
            "resolution": "THE_400_P"
        },
        "processing": {
            "april": {
                "useApril": false,
                "resolution": [320, 180],
                "tagFamily": "TAG_36H11",
                "config": {
                    "quadDecimate": 1,
                    "quadSigma": 0,
                    "refineEdges": true,
                    "decodeSharpening": 0.25,
                    "maxHammingDistance": 1,
                    "quadThresholds": {
                        "minClusterPixels": 5,
                        "maxNmaxima": 15,
                        "criticalDegree": 10,
                        "maxLineFitMse": 10,
                        "minWhiteBlackDiff": 5,
                        "deglitch": false
                    }
                }
            },
            "nn": {
                "useNN": "mobilenet_ssd"
            }
        }
    }
}
